    "n = int(input())\n",
    "\n",
    "dp=[0]*(31)\n",
    "\n",
    "dp[2]=3\n",
    "for i in range(4,n+1,2):\n",
    "    dp[i]=dp[i-2]*3\n",
    "    for j in range(2,i-2,2):\n",
    "        dp[i]+=dp[j]*2\n",
    "    dp[i]+=2\n",
    "print(dp[n])"
